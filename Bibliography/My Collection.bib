isbn = {1364-0321},
journal = {Renewable \& Sustainable Energy Reviews},
number = {1},
pages = {413--421},
title = {{Strategic selection of suitable projects for hybrid solar-wind power generation systems}},
url = {<Go to ISI>://000271279100029},
volume = {14},
year = {2010}
