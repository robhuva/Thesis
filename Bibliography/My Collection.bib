file = {:home/rhuva/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/McVicar et al. - 2008 - Wind speed climatology and trends for Australia, 1975-2006 Capturing the stilling phenomenon and comparison with near-surface reanalysis output.pdf:pdf},
isbn = {0094-8276},
