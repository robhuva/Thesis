isbn = {0916-8370},
journal = {Journal of Oceanography},
number = {1},
pages = {53--60},
title = {{The Southern Annular Mode and opposite-phased Basin Mode of the Southern Ocean circulation}},
url = {<Go to ISI>://WOS:000261957900005},
volume = {65},
year = {2009}
}
