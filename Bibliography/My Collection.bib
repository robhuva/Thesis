pages = {352--360},
publisher = {American Meteorological Society},
title = {{A caveat concerning singular value decomposition}},
url = {http://cat.inist.fr/?aModele=afficheN\&amp;cpsidt=3451791},
volume = {8},
year = {1995}
}
