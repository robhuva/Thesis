journal = {Energy},
keywords = {energy system design,large-scale integration of renewable,power generation,power transmission,pv power generation,solar,wind power generation},
pages = {443--458},
title = {{Optimized mixes of wind and solar PV and transmission grid extensions}},
volume = {72},
year = {2014}
}
