keywords = {SOM,applied climatology,artificial,climate-change,forced hydroclimatology,general-circulation models,intraseasonal variability,major arctic watersheds,neural-networks,part 1,polar-regions,self-organizing map,southwest western-australia,synoptic climatology,weather patterns},
language = {English},
