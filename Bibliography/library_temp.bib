@article{Stephens2010,
abstract = {To the Editor: In her News Feature \{“Biotech's\} green gold”, Emily Waltz details the 'hype' being propagated around emerging microalgal biofuel technologies, which often exceeds the physical and thermodynamic constraints that ultimately define their economic viability. Our calculations (Supplementary Box 1) counter such excessive claims and demonstrate that 22 \{MJ\} m−2 d−1 solar radiation supports practical yield maxima of \{$\backslash$textbackslash\}textasciitilde60 to 100 kl oil ha−1 y−1 (\{$\backslash$textbackslash\}textasciitilde6,600 to 10,800 gal ac−1 y−1) and an absolute theoretical ceiling of \{$\backslash$textbackslash\}textasciitilde94 to 155 kl oil ha−1 y−1, assuming a maximum photosynthetic conversion efficiency of 10\% (ref. 3) (results summarized in Table 1). To evaluate claims and provide an accurate analysis of the potential of microalgal biofuel systems, we have conducted industrial feasibility studies and sensitivity analyses based on peer-reviewed data and industrial expertise. Given that microalgal biofuel research is still young and its development still in flux, we anticipate that the stringent assessment of the technology's economic potential presented below will assist \{R\&D\} investment and policy development in the area going forward. View full text},
author = {Stephens, Evan and Ross, Ian L and King, Zachary and Mussgnug, Jan H and Kruse, Olaf and Posten, Clemens and Borowitzka, Michael a and Hankamer, Ben},
doi = {10.1038/nbt0210-126},
