@article{Timbal2013,
abstract = {During the 20th century and early 21st century, the subtropical ridge (STR) in the vicinity of eastern Australia has intensified significantly. While the position of the STR has often been the focus of past studies, its influence on rainfall across southern Australia is less pronounced than the influence of the intensity of the ridge. Most of the Australian continent has experienced above average rainfall from 1997 to 2009, but areas of below average rainfall, including southeastern Australia (SEA), exist and tend to match the pattern of the influence of the STR on Australian rainfall. The spatial extent makes the 1997–2009 rainfall deficit in SEA different from previous dry decades such as 1935–1945. It is also noted that the annual cycle of the rainfall deficiency centred on a continuum from March to October overlaps well with the time of the year when the STR intensity has a strong influence on rainfall. Using simple linear statistics, a rainfall decline for the period 1997–2009 equivalent to nearly two thirds of that observed can be inferred from the intensification of the ridge. The apparent southward shift of the ridge in certain seasons does not appear to have had an additional effect on SEA rainfall. During the 1935–1945 drought, almost a third of the rainfall decline can be attributed to the strengthening of the ridge. Finally, it was observed that the intensification of the STR was not monotonic during the 20th century but happened mostly during two extended periods: from 1900 to the 1940s, culminating at the time of the 1935–1945 dry decade, and from 1970 to 2010 culminating with the 1997–2009 rainfall deficit in SEA. That multidecadal behaviour is reminiscent of the global warming of the planet.},
author = {Timbal, B. and Drosdowsky, W.},
doi = {10.1002/joc.3492},
file = {:home/rhuva/Documents/Journals and Resources/Synoptic Analysis \& ERA/SE Aus rainfall decline and the STR, Timbal-2013.pdf:pdf},
issn = {08998418},
journal = {International Journal of Climatology},
keywords = {Rainfall,South East Australia,Subtropical ridge},
month = mar,
number = {4},
pages = {1021--1034},
title = {{The relationship between the decline of Southeastern Australian rainfall and the strengthening of the subtropical ridge}},
url = {http://doi.wiley.com/10.1002/joc.3492},
volume = {33},
year = {2013}
}
@article{Timmerman2008,
abstract = {For optimal energy conversion in photovoltaic devices (electricity$\backslash$nto and from light) one important requirement is that the full energy$\backslash$nof the photons is used. However, in solar cells, a single electron-hole$\backslash$npair of specific energy is generated when the incoming photon energy$\backslash$nis above a certain threshold, with the excess energy being lost to$\backslash$nheat. In the so-called quantum-cutting process, a high-energy photon$\backslash$ncan be divided into two, or more, photons of lower energy. Such manipulation$\backslash$nof photon quantum size can then very effectively increase the overall$\backslash$nefficiency of a device. In the current work, we demonstrate (space-separated)$\backslash$nphoton cutting by silicon nanocrystals, in which nearby Er3+ ions$\backslash$nand neighbouring nanocrystals are used to detect this effect.},
