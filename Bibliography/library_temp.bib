isbn = {0094-8276},
issn = {00948276},
journal = {Geophysical Research Letters},
number = {20},
title = {{Wind speed climatology and trends for Australia, 1975-2006: Capturing the stilling phenomenon and comparison with near-surface reanalysis output}},
url = {<Go to ISI>://000260177400006},
