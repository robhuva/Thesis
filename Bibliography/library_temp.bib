journal = {Atmospheric Research},
keywords = {General circulation model,North Atlantic Oscillation,North Sea-Caspian Pattern,Principal Component Analysis,Reanalysis data,Self Organizing Maps},
month = nov,
pages = {123--137},
publisher = {Elsevier B.V.},
title = {{Representing teleconnection patterns over Europe: A comparison of SOM and PCA methods}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0169809513003311},
volume = {152},
year = {2013}
}
