}
@book{Holton2004,
abstract = {The general circulation of the atmosphere includes the totality of motions that characterize the global-scale atmospheric flow. The study of the general circulation is concerned with the dynamics of climate—that is, with the temporally averaged structures of the fields of wind, temperature, humidity, precipitation, and other meteorological variables. The general circulation is thus considered to consist of the flow averaged in time over a period sufficiently long to remove the random variations associated with individual weather systems but short enough to retain monthly and seasonal variations. This chapter focuses on the influence of eddies on the structure of the zonal-mean circulation and discusses the angular momentum and energy budgets of the zonally averaged flow. The longitudinally varying time averaged circulation is also reviewed and the gross features of the general circulation outside the tropics are also discussed on the basis of quasi-geostrophic theory.},
address = {Burlington, MA, USA},
