@article{Dowdy2013,
abstract = {Extratropical cyclones that develop near the east coast of Australia$\backslash$noften have severe consequences such as flash flooding and damaging$\backslash$nwinds and seas, as well as beneficial consequences such as being$\backslash$nresponsible for heavy rainfall events that contribute significantly$\backslash$nto total rainfall and runoff. There is subjective evidence that the$\backslash$ndevelopment of most major events, commonly known as East Coast Lows$\backslash$n(ECLs), is associated with the movement of a high amplitude upper-tropospheric$\backslash$ntrough system over eastern Australia. To test this hypothesis, this$\backslash$nstudy examines a number of large-scale diagnostic quantities in the$\backslash$nupper-troposphere associated with extratropical cyclogenesis. Climatologies$\backslash$nof these diagnostic quantities, based on reanalyses, are examined$\backslash$nand compared with a database of observed ECL events. Results are$\backslash$nexamined in relation to seasonal and geographic variations. A diagnostic$\backslash$nquantity based on geostrophic vorticity is shown to provide a good$\backslash$nindication of the likely occurrence of east-coast cyclogenesis. The$\backslash$npotential application of these diagnostics to global climate model$\backslash$nsimulations of past and future climates is also discussed.},
