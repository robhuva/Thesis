@article{Speer2010,
abstract = {The New South Wales (NSW) coast is subject to heavy rain, strong wind$\backslash$nand large waves resulting from low pressure systems over the adjacent$\backslash$nTasman Sea that develop from a variety of synoptic and mesoscale$\backslash$nmechanisms. A database of these maritime lows and their impacts has$\backslash$nbeen developed in the NSW Climate Services Centre of the Bureau of$\backslash$nMeteorology. The database currently extends back to 1970 and includes$\backslash$ndata on rainfall amounts, with wind speed and significant wave height$\backslash$ndata still to be added. The database events were classified into$\backslash$nsix synoptic types based on the mean sea level pressure synoptic$\backslash$npattern in which the lows formed. The six types are: inland trough$\backslash$nlows (30 per cent), easterly trough lows (14 per cent), and ex-tropical$\backslash$ncyclones (4 per cent), all of which originate in the subtropical$\backslash$nor tropical easterlies; and, lows forming on a wave on a front (37$\backslash$nper cent) decaying front lows (12 per cent) and lows in the westerlies$\backslash$n(3 per cent), the latter two of which originate from mid-latitude$\backslash$nlow pressure systems or fronts in the westerlies. Since 1970, only$\backslash$ninland trough lows have shown a significant increase in frequency$\backslash$nwhich is consistent with a slight increase in spring rainfall in$\backslash$nan area over northeast NSW over the same period. In contrast, there$\backslash$nhas been a decrease in ex-tropical cyclone numbers impacting the$\backslash$nNSW coast since 1970, which is consistent with a decrease in summer$\backslash$nrainfall generally along the NSW coast. The development of the database$\backslash$nis ongoing but it is planned to extend it back in time to further$\backslash$ninvestigate the relationship between maritime low pressure development$\backslash$nand NSW coastal rainfall trends.},
author = {Speer, M S and Wiles, P and Pepler, A},
file = {:home/rhuva/Documents/Journals and Resources/Synoptic Analysis \& ERA/Low pressure systems synoptic typing and NSW weather, Speer-2009.pdf:pdf},
issn = {1836716X},
journal = {Australian Meteorological and Oceanographic Journal},
number = {2009},
pages = {29--39},
title = {{Low pressure systems off the New South Wales coast and associated hazardous weather: establishment of a database}},
volume = {58},
year = {2009}
}
@article{Timbal2010,
abstract = {The Eastern SeaBoard (ESB) of Australia has long been recognised as a separate climate entity. Using the latest gridded observations from the Bureau of Meteorology, a definition of the spatial extent of the ESB is proposed. It appears that, while this area has recorded below average rainfall over the last 12 years, the ongoing deficiency is not record breaking in historic terms. This contrasts with record breaking droughts across large parts of inland, eastern Australia. The lesser severity of ongoing rainfall deficiencies in the ESB, compared to the rest of the region, is linked to the different impact of observed changes in regional surface pressure and, in particular, changes in the position of the sub-tropical ridge. It is also observed that while tropical modes of variability in the Pacific and Indian oceans are known to influence the climate of eastern Australia, that influence appears very weak and not statistically significant across the ESB. Finally, some issues relevant to future rainfall projections for the ESB are discussed. It is argued that providing reliable climate projections across this climatic region is a difficult challenge.},
author = {Timbal, Bertrand},
doi = {10.1088/1755-1315/11/1/012013},
file = {:home/rhuva/Documents/Journals and Resources/Synoptic Analysis \& ERA/Climate of SE Aus difficulties and projections, Timbal-2010.pdf:pdf},
issn = {1755-1315},
journal = {IOP Conference Series: Earth and Environmental Science},
month = aug,
pages = {012013},
title = {{The climate of the Eastern Seaboard of Australia: A challenging entity now and for future projections}},
url = {http://stacks.iop.org/1755-1315/11/i=1/a=012013?key=crossref.0ea326f34a85c496e2a182cc6c4649c6},
volume = {11},
year = {2010}
}
