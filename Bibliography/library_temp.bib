journal = {Applied Energy},
keywords = {Analyses,Initial and boundary conditions,Reanalyses,WRF,Wind energy,Wind modeling},
pages = {116--126},
publisher = {Elsevier Ltd},
title = {{WRF wind simulation and wind energy production estimates forced by different reanalyses: Comparison with observed data for Portugal}},
